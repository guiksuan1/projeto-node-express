<%- include ('partials/header.ejs') %>
    <%- include ('partials/navbar.ejs') %>

        <div class="container">
            <div class="card">
                <div class="card-header">
                    <h1>Musicas</h1>
                </div>
                <div class="card-body">
                    <table>
                        <thead>
                            <tr>
                                <th></th>
                                <th>Nome:</th>
                                <th>Ano:</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% Musicas.forEach(Musicas=> { %>
                                <tr>
                                    <td><img src="imgs/logo.jpeg" width="30px"></td>
                                    <td>
                                        <%= Musicas.nome %>
                                    </td>
                                    <td>
                                        <%= Musicas.ano %>
                                    </td>
                                </tr>
                                <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <%- include ('partials/footer.ejs') %>